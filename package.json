{
  "name": "npac",
  "description": "A lightweight Ports and Adapters Container for Node",
  "version": "1.1.1",
  "tags": [
    "es6",
    "module",
    "npac"
  ],
  "author": {
    "name": "Tam√°s Benke",
    "email": "tombenke@gmail.com"
  },
  "bugs": {
    "url": "https://github.com/tombenke/npac/issues"
  },
  "license": "MIT",
  "homepage": "https://github.com/tombenke/npac",
  "repository": {
    "type": "git",
    "url": "http://github.com/tombenke/npac.git"
  },
  "engines": {
    "node": "^6.11.2"
  },
  "main": "dist/index.js",
  "dependencies": {
    "async": "^2.6.0",
    "datafile": "^3.2.0",
    "lodash": "^4.17.4",
    "winston": "^3.0.0-rc1"
  },
  "devDependencies": {
    "babel-cli": "6.16.0",
    "babel-core": "6.17.0",
    "babel-loader": "6.2.5",
    "babel-plugin-transform-es2015-block-scoping": "^6.26.0",
    "babel-plugin-transform-es2015-parameters": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "6.20.2",
    "babel-polyfill": "6.16.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-latest": "6.16.0",
    "babel-register": "6.16.3",
    "coveralls": "^2.11.15",
    "cross-env": "^2.x.x",
    "eslint": "^3.13.1",
    "expect": "^1.6.0",
    "istanbul": "1.0.0-alpha.2",
    "jsdoc": "^3.5.3",
    "mocha": "^2.x.x",
    "mocha-lcov-reporter": "^1.2.0",
    "pre-push": "^0.1.1",
    "rimraf": "^2.6.2"
  },
  "scripts": {
    "dev": "babel src --presets babel-preset-es2015 --out-dir dist --watch",
    "build": "babel src --presets babel-preset-es2015 --out-dir dist",
    "lint": "eslint src/**.js",
    "test": "cross-env NODE_ENV=test mocha --recursive --compilers js:babel-register 'src/**/*.spec.js'",
    "test:watch": "npm test -- --watch",
    "coverage": "NODE_ENV=test istanbul cover _mocha -- recursive --compilers js:babel-register --timeout 15000 'src/**/*.spec.js'",
    "coveralls": "NODE_ENV=test istanbul cover _mocha --report lcovonly -- recursive --compilers js:babel-register --timeout 15000 'src/**/*.spec.js' -R spec && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage",
    "docs": "rm -fr ./docs/* && jsdoc -c ./.jsdoc.json",
    "prepublish": "npm run lint && npm run test && npm run build && npm run docs",
    "all": "npm run lint && npm run coverage && npm run build && npm run docs"
  },
  "pre-push": [
    "lint",
    "test",
    "prepublish"
  ]
}
